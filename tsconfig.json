{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": [
      "jest",
      "@testing-library/jest-dom"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@api": [
        "./@next/api"
      ],
      "@types": [
        "./@next/types"
      ],
      "@shared/*": [
        "./@next/shared/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/locale/*": [
        "./@next/locale/*"
      ],
      "@/middlewares/*": [
        "./@next/middlewares/*"
      ],
      "@grpc/*": [
        "./@next/grpc/*"
      ],
      "@protos/*": [
        "./src/proto/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@utils": [
        "./@next/utils"
      ],
      "@ApiReq": [
        "./@next/ApiReq"
      ],
      "@hooks": [
        "./@next/hooks"
      ],
      "@config": [
        "./@next/config"
      ],
      "@store": [
        "./@next/store"
      ],
      "@atoms": [
        "./@next/components/atoms"
      ],
      "@molecules": [
        "./@next/components/molecules"
      ],
      "@organisms": [
        "./@next/components/organisms"
      ],
      "@templates": [
        "./@next/components/templates"
      ],
      "@services": [
        "./@next/services"
      ],
      "@pages": [
        "./@next/components/pages"
      ],
      "@svgs": [
        "./@next/svgs"
      ],

      "@layouts": [
        "./@next/layouts"
      ],
      "test-utils": [
        "./@next/utils/test.utils.tsx"
      ],
      "@mocks/*": [
        "./__mocks__/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types.d.ts",
    "next.config.js",
    "tailwind.config.js",
    "postcss.config.js",
    ".next/types/**/*.ts",
    "./jest-setup.js",
    "./jest-config.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
